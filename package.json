  "scripts": {
    "dev": "run-p start webrtc",
    "start": "CHOKIDAR_USEPOLLING=true react-app-rewired start",
    "start-chrome": "CHOKIDAR_USEPOLLING=true BROWSER=chrome react-app-rewired start",
    "start-https-chrome": "CHOKIDAR_USEPOLLING=true BROWSER=chrome HTTPS=true react-app-rewired --openssl-legacy-provider start",
    "build": "BROWSER=chrome react-app-rewired -max-old-space-size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "gate-ipfs": "ipfs daemon",
    "gate-db": "sudo mongod --dbpath ./data/gatedb --port 27018",
    "gate-ganache": "nodemon --ignore ./NFTProtocol/V2/gatewayV2/gatenft/ --ignore ./NFTProtocol/V2/gatewayV2/leaves --ignore ./src --ignore ./NFTProtocol/V2/oracleV2 ./NFTProtocol/V2/gatewayV2/gateway.js Ganache",
    "gate-mumbai": "nodemon --ignore ./NFTProtocol/V2/gatewayV2/gatenft/ --ignore ./NFTProtocol/V2/gatewayV2/leaves --ignore ./src --ignore ./NFTProtocol/V2/oracleV2 ./NFTProtocol/V2/gatewayV2/gateway.js Mumbai",
    "oracle-ipfs": "jsipfs daemon",
    "oracle-db": "sudo mongod --dbpath ./data/db",
    "oracle-ganache": "nodemon --ignore ./NFTProtocol/V2/oracleV2/nft --ignore ./NFTProtocol/V2/oracleV2/leaves --ignore ./src --ignore ./NFTProtocol/V2/gatewayV2 ./NFTProtocol/V2/oracleV2/oracle.js Ganache HTTP",
    "oracle-mumbai": "nodemon --ignore ./NFTProtocol/V2/oracleV2/nft --ignore ./NFTProtocol/V2/oracleV2/leaves --ignore ./src --ignore ./NFTProtocol/V2/gatewayV2 ./NFTProtocol/V2/oracleV2/oracle.js Mumbai HTTP",
    "contractsTest": "truffle test ./test/MockGovernor.js --compile-none",
    "cryptoGameTruffle": "truffle test ./test/CryptoGame.js",
    "cryptoMonkey": "tsc ./CryptoMonkeys/CryptoMonkeysVRF.ts",
    "cryptoGameTest": "run-s cryptoGameTruffle",
    "nftProtocol": "truffle test ./test/NFTProtocol.js",
    "ipfs-execute": "node ./IPFS/ipfs-execute.js",
    "deploy-ganache": "node --no-deprecation ./deployment/deploy.js Ganache NFTProtocol NFTMinter CryptoMonkeys",
    "deploy-mumbai": "node --no-deprecation ./deployment/deploy.js Mumbai NFTProtocol",
    "migrate": "truffle migrate --network development --compile-none",
    "ganache": "bash ganache.sh",
    "webrtc": "webrtc-star --port 13579",
    "ipfs-testing": "nodemon --ignore ./src --ignore package.json ./IPFS/ipfs-testing.js",
    "nftTesting": " node ./NFTProtocol/test/V2/nftProtocolTestsV2.js",
    "nftTest-ganache": "run-s deploy-ganache nftTesting",
    "nftTest-mumbai": "run-s deploy-mumbai nftTesting",
    "nftGateTest": "node ./NFTProtocol/test/V2/nftGatewayTesting.js",
    "nftOracleTesting": "node ./NFTProtocol/test/V2/nftOracleTesting.js",
    "nftOracleTest": "run-s deployTest nftOracleTesting",
    "init": "node ./init/init.js",
    "oracle": "node --loader ts-node/esm ./Oracle/Oracle.ts",
    "cryptoMonkeys": "node --loader ts-node/esm ./CryptoMonkeys/CryptoMonkeysVRF.ts Ganache",
    "cryptoMonkeysTest": "node --loader ts-node/esm ./CryptoMonkeys/test/CryptoMonkeysTest.ts Ganache",
    "cmtest": "run-s deploy-ganache cryptoMonkeysTest"
